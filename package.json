{
  "name": "@typescene/webapp",
  "version": "2.0.4",
  "description": "Typescene Web Application toolkit",
  "author": "Jelmer Cormont",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/typescene/typescene-webapp.git"
  },
  "bugs": {
    "url": "https://github.com/typescene/typescene-webapp/issues"
  },
  "homepage": "http://typescene.org/",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "typings": "./typings/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist typings",
    "prepublishOnly": "npm run build",
    "prebuild": "rm -rf dist typings && tsc -p src --declaration --declarationDir typings",
    "build": "tsc -p src --removeComments",
    "start": "cd demo && parcel index.html --open"
  },
  "dependencies": {
    "tslib": "^1.9.3",
    "typescene": "^2.11.5"
  },
  "devDependencies": {
    "@types/feather-icons": "^4.7.0",
    "feather-icons": "^4.10.0",
    "parcel-bundler": "^1.11.0",
    "typescript": "^3.2.2"
  }
}
