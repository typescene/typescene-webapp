    "start": "cd demo && parcel index.html --open"
  },
  "dependencies": {
    "tslib": "^1.9.3",
    "typescene": "^2.12.1"
  },
  "devDependencies": {
    "rimraf": "^2.6.3",
    "@types/feather-icons": "^4.7.0",
    "feather-icons": "^4.10.0",
    "parcel-bundler": "^1.11.0",
    "typescript": "^3.4.5"
  }
